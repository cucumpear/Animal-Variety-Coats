<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!--=====================  Warg ========================-->

	<Operation Class="PatchOperationAdd">
	  <xpath>*/ThingDef[defName = "Warg"]</xpath>
	  <value>
		<thingClass>AnimalVariations.AnimalMultiSkins</thingClass>
	  </value>
	</Operation>
	<Operation Class="PatchOperationReplace">
	  <xpath>*/PawnKindDef[defName = "Warg"]/lifeStages</xpath>
	  <value>
    <lifeStages>
      <li>
        <label>warg puppy</label>
        <labelPlural>warg puppies</labelPlural>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Warg/Warg</texPath>
          <drawSize>1.05</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
		  <texPath>Things/Pawn/Animal/Dessicated/Dessicated_Warg</texPath>
          <drawSize>1</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Warg/Warg</texPath>
          <drawSize>1.7</drawSize>
          <shadowData>
            <volume>(0.25, 0.3, 0.25)</volume>
            <offset>(0,0,-0.15)</offset>
          </shadowData>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
		  <texPath>Things/Pawn/Animal/Dessicated/Dessicated_Warg</texPath>
          <drawSize>1.4</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Warg/Warg</texPath>
          <drawSize>2.05</drawSize>
          <shadowData>
            <volume>(0.3, 0.4, 0.3)</volume>
            <offset>(0,0,-0.15)</offset>
          </shadowData>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
		  <texPath>Things/Pawn/Animal/Dessicated/Dessicated_Warg</texPath>
          <drawSize>1.7</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>
	  </value>
	</Operation>
