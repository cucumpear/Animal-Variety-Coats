	<!--=====================  Squirrel ========================-->

	<Operation Class="PatchOperationAdd">
	  <xpath>*/ThingDef[defName = "Squirrel"]</xpath>
	  <value>
		<thingClass>AnimalVariations.AnimalMultiSkins</thingClass>
	  </value>
	</Operation>
	<Operation Class="PatchOperationReplace">
	  <xpath>*/PawnKindDef[defName = "Squirrel"]/lifeStages</xpath>
	  <value>
		<lifeStages>
